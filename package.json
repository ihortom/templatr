{
  "name": "templatr",
  "productName": "Templatr",
  "version": "1.1.0",
  "description": "Electron-React boilerplate",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihortom/templatr.git"
  },
  "scripts": {
    "bundle": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "start": "export ENV=DEV && electron .",
    "package-mac": "export ENV=PROD && sudo electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/templatr.icns --prune=true --out=builds",
    "create-installer-mac": "sudo electron-installer-dmg builds/Templatr-darwin-x64/Templatr.app Templatr_v1.1.0 --title='Templatr 1.1.0' --out=builds --overwrite --icon=assets/templatr.icns --background=assets/installer.png --icon-size=100"
  },
  "keywords": [],
  "author": "Ihor Tomilenko",
  "license": "ISC",
  "dependencies": {
    "electron-context-menu": "^3.1.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.2.0",
    "uuid": "^8.3.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@webpack-cli/serve": "^1.4.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.4",
    "electron": "^13.1.4",
    "electron-installer-dmg": "^3.0.0",
    "electron-reload": "^1.5.0",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "bugs": {
    "url": "https://github.com/ihortom/templatr/issues"
  }
}
